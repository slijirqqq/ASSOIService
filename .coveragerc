[run]
branch = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    class UserManager
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @(abc\.)?abstractmethod

omit =
    manage.py

ignore_errors = True
